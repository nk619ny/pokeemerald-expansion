script NewTrainerHill_EventScript_SetupAudienceCount {
	if (flag(FLAG_IS_CHAMPION)) {
		setvar( VAR_TEMP_F, DOME_FINAL)
	}
	elif (flag(FLAG_BADGE06_GET)) {
		setvar( VAR_TEMP_F, DOME_SEMIFINAL)
	}
	elif (flag(FLAG_BADGE03_GET)) {
		setvar( VAR_TEMP_F, DOME_ROUND2)
	}
	else {
		setvar( VAR_TEMP_F, DOME_ROUND1)
	}
	return
}

script NewTrainerHill_EventScript_AnnouncePlayer {
	switch (var(VAR_TEMP_F)) {
		case DOME_ROUND1:
			msgbox("Presenting a local trainer from Littleroot Town: {PLAYER}!", MSGBOX_DEFAULT)
		case DOME_ROUND2:
			msgbox("Presenting a rising talent who has been collecting Gym Badges: {PLAYER}!", MSGBOX_DEFAULT)
		case DOME_SEMIFINAL:
			msgbox("Presenting a fierce competitor on his way to Ever Grande City: {PLAYER}!", MSGBOX_DEFAULT)
		case DOME_FINAL:
			msgbox("Presenting {PLAYER}, a Hoenn League Champion!", MSGBOX_DEFAULT)
	}
	return
}

script NewTrainerHill_EventScript_SetOpponent {
	switch (var(VAR_TRAINER_HILL_STATE)) {
		case 1:
			setvar( VAR_OBJ_GFX_ID_0, OBJ_EVENT_GFX_YOUNGSTER)
		case 2:
			setvar( VAR_OBJ_GFX_ID_0, OBJ_EVENT_GFX_BEAUTY)
		case 3:
			setvar( VAR_OBJ_GFX_ID_0, OBJ_EVENT_GFX_SCIENTIST_1)
		case 4:
			setvar( VAR_OBJ_GFX_ID_0, OBJ_EVENT_GFX_EXPERT_F)
	}
	return
}

script NewTrainerHill_EventScript_AnnounceOpponent {
	switch (var(VAR_TRAINER_HILL_STATE)) {
		case 1:
			msgbox(format("{PLAYER} will be facing Charlie, a Youngster from Dewford Town!"), MSGBOX_NPC)
		case 2:
			msgbox(format("{PLAYER} will be facing Lily, a Beauty from Slateport City!"), MSGBOX_NPC)
		case 3:
			msgbox(format("{PLAYER} will be facing Max, a Scientist from Mauville City!"), MSGBOX_NPC)
		case 4:
			msgbox(format("{PLAYER} will be facing Flora, an Expert from Lilycove City!"), MSGBOX_NPC)
	}
	return
}

script NewTrainerHill_EventScript_StartBattle {
	switch (var(VAR_TRAINER_HILL_STATE)) {
		case 1:
			trainerbattle_no_intro(TRAINER_HILL_1, LilycoveCity_Text_BrendanDefeat)
		case 2:
			trainerbattle_no_intro(TRAINER_HILL_2, LilycoveCity_Text_BrendanDefeat)
		case 3:
			trainerbattle_no_intro(TRAINER_HILL_3, LilycoveCity_Text_BrendanDefeat)
		case 4:
			trainerbattle_no_intro(TRAINER_HILL_4, LilycoveCity_Text_BrendanDefeat)
	}
	return
}

script NewTrainerHill_EventScript_NewBattleSequence {
	call(NewTrainerHill_EventScript_AnnounceOpponent)
	closemessage
	applymovement(LOCALID_DOME_PLAYER, BattleFrontier_BattleDomeBattleRoom_Movement_PlayerStepForward)
	applymovement(LOCALID_DOME_OPPONENT, BattleFrontier_BattleDomeBattleRoom_Movement_OpponentStepForward)
	applymovement(LOCALID_PLAYER, Common_Movement_WalkDownOneStep)
	waitmovement(0)
	delay(60)
	call(NewTrainerHill_EventScript_StartBattle)
	addvar(VAR_TRAINER_HILL_STATE, 1)
	warp(MAP_TRAINER_HILL_ENTRANCE, 5, 11)
	return
}