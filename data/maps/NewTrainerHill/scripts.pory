	delay_8
	disable_jump_landing_ground_effect
	jump_in_place_down
	delay_4
	jump_in_place_down
	enable_jump_landing_ground_effect
	step_end

NewTrainerHill_Movement_AudienceMemberWalkToSeat:
	walk_down
