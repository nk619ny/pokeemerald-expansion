script MauvilleCity_GameCorner_EventScript_Scott {
	lock
//	if(flag(FLAG_TEMP_8)){
//		goto(MauvilleCity_GameCorner_EventScript_ScottRepeat)
//	}
	faceplayer
	msgbox(MauvilleCity_GameCorner_Text_ScottIntro,MSGBOX_NPC)
	msgbox(format("Are you interested?"),MSGBOX_YESNO)
	if(var(VAR_RESULT) == YES){
		msgbox(MauvilleCity_GameCorner_Text_ScottAgree,MSGBOX_NPC)
		giveitem(ITEM_COIN_CASE)
		msgbox(format("Scott: I look forward to seeing what you can do!"),MSGBOX_NPC)
	}
	else{
		msgbox(MauvilleCity_GameCorner_Text_ScottDecline,MSGBOX_NPC)
		giveitem(ITEM_COIN_CASE)
		msgbox(format("Scott: I hope to see you around!"),MSGBOX_NPC)
	}
	waitmessage
	setflag(FLAG_RECEIVED_COIN_CASE)
	switch(var(VAR_FACING)){
		case DIR_NORTH:
			applymovement(LOCALID_GAMECORNER_SCOTT, MauvilleCity_GameCorner_Movement_ScottLeaveLeft)
		case DIR_EAST:
			applymovement(LOCALID_GAMECORNER_SCOTT, MauvilleCity_GameCorner_Movement_ScottLeave)
		case DIR_WEST:
			applymovement(LOCALID_GAMECORNER_SCOTT, MauvilleCity_GameCorner_Movement_ScottLeave)
	}
	waitmovement(0)
	removeobject(LOCALID_GAMECORNER_SCOTT)
	releaseall
	end
}

