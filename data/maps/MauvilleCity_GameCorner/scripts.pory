script MauvilleCity_GameCorner_EventScript_ExchangeCoinsForPrizes {
	lock
	faceplayer
	checkitem(ITEM_COIN_CASE)
	if(var(VAR_RESULT) == FALSE){
		msgbox(format("I'm sorry, but you need a Coin Case to purchase prizes."), MSGBOX_DEFAULT)
		closemessage
		releaseall
		end
	}
	checkcoins(TempVar_CurrentCoins)
	msgbox(format("Which kind of prize would you like to purchase?"), MSGBOX_DEFAULT)
	dynmultichoice(25,2,FALSE,5,0,DYN_MULTICHOICE_CB_NONE, "One-Time Items", "Held Items", "Technical Machines", "Rare Battle Items", "EV Vitamins", "Type Gems")
	switch(var(VAR_RESULT)) {
		case 0:
			goto(MauvilleCity_GameCorner_EventScript_BuyOneTimeHeldItems)
		case 1:
			goto(MauvilleCity_GameCorner_EventScript_BuyPermanentHeldItems)
		case 2:
			goto(MauvilleCity_GameCorner_EventScript_BuyTMs)
		case 3:
			goto(MauvilleCity_GameCorner_EventScript_BuyRareBattleItems)
		case 4:
			goto(MauvilleCity_GameCorner_EventScript_BuyVitamins)
		case 5: 
			goto(MauvilleCity_GameCorner_EventScript_BuyGems)
		default:
			goto(MauvilleCity_GameCorner_EventScript_ComeBackLater)
	}
	closemessage
	releaseall
	end
}

