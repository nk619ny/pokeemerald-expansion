    if(!flag(FLAG_RECEIVED_LIFE_ORB)) {dynmultipush("Life Orb",ITEM_LIFE_ORB)}
    if(!flag(FLAG_RECEIVED_ASSAULT_VEST)) {dynmultipush("Assault Vest",ITEM_ASSAULT_VEST)}
	msgbox(format("Which item would you like for 1,250 coins?"), MSGBOX_DEFAULT)
	showcoinsbox(1, 1)
	dynmultistack(25, 0, FALSE, 6, FALSE, 0, DYN_MULTICHOICE_CB_NONE)
	copyvar(TempVar_PrizeToBuy, VAR_RESULT)
	bufferitemname(STR_VAR_1, TempVar_PrizeToBuy)
	switch(var(TempVar_PrizeToBuy)) {
	    case ITEM_CHOICE_BAND:          
            msgbox(format("You want to buy {STR_VAR_1} for {STR_VAR_2} coins. Is that correct?"), MSGBOX_YESNO)
            goto_if_eq(VAR_RESULT, NO, MauvilleCity_GameCorner_EventScript_ComeBackLater)
            removecoins(1250)
            additem(TempVar_PrizeToBuy)
            setflag(FLAG_RECEIVED_CHOICE_BAND)
        case ITEM_CHOICE_SCARF:
            msgbox(format("You want to buy {STR_VAR_1} for {STR_VAR_2} coins. Is that correct?"), MSGBOX_YESNO)
            goto_if_eq(VAR_RESULT, NO, MauvilleCity_GameCorner_EventScript_ComeBackLater)
            removecoins(1250)
            additem(TempVar_PrizeToBuy)
            setflag(FLAG_RECEIVED_CHOICE_SCARF)
        case ITEM_CHOICE_SPECS:
            msgbox(format("You want to buy {STR_VAR_1} for {STR_VAR_2} coins. Is that correct?"), MSGBOX_YESNO)
            goto_if_eq(VAR_RESULT, NO, MauvilleCity_GameCorner_EventScript_ComeBackLater)
            removecoins(1250)
            additem(TempVar_PrizeToBuy)
            setflag(FLAG_RECEIVED_CHOICE_SPECS)
        case ITEM_LEFTOVERS:
            msgbox(format("You want to buy {STR_VAR_1} for {STR_VAR_2} coins. Is that correct?"), MSGBOX_YESNO)
            goto_if_eq(VAR_RESULT, NO, MauvilleCity_GameCorner_EventScript_ComeBackLater)
            removecoins(1250)
            additem(TempVar_PrizeToBuy)
            setflag(FLAG_RECEIVED_LEFTOVERS)
        case ITEM_LIFE_ORB:
            msgbox(format("You want to buy {STR_VAR_1} for {STR_VAR_2} coins. Is that correct?"), MSGBOX_YESNO)
            goto_if_eq(VAR_RESULT, NO, MauvilleCity_GameCorner_EventScript_ComeBackLater)
            removecoins(1250)
            additem(TempVar_PrizeToBuy)
            setflag(FLAG_RECEIVED_LIFE_ORB)
        case ITEM_ASSAULT_VEST:
            msgbox(format("You want to buy {STR_VAR_1} for {STR_VAR_2} coins. Is that correct?"), MSGBOX_YESNO)
            goto_if_eq(VAR_RESULT, NO, MauvilleCity_GameCorner_EventScript_ComeBackLater)
            removecoins(1250)
            additem(TempVar_PrizeToBuy)
            setflag(FLAG_RECEIVED_ASSAULT_VEST)
		default:
			hidecoinsbox(0, 5)
			goto(MauvilleCity_GameCorner_EventScript_ComeBackLater)
	}
	closemessage
	playse(SE_SHOP)
