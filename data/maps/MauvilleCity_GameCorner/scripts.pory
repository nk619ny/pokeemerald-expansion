script MauvilleCity_GameCorner_EventScript_BuyRareBattleItems{
	if(flag(FLAG_RECEIVED_CHOICE_BAND) && flag(FLAG_RECEIVED_CHOICE_SCARF) && flag(FLAG_RECEIVED_CHOICE_SPECS) && flag(FLAG_RECEIVED_LEFTOVERS) && flag(FLAG_RECEIVED_LIFE_ORB) && flag(FLAG_RECEIVED_ASSAULT_VEST)){
		goto(MauvilleCity_GameCorner_EventScript_OutOfRareItems)
	}
	checkcoins(TempVar_CurrentCoins)
	goto_if_lt(TempVar_CurrentCoins, 1250, MauvilleCity_GameCorner_EventScript_NotEnoughCoinsForRarePrize)
	msgbox(format("We only have one of each of these items, and they cost a whopping 1,250 coins each."), MSGBOX_DEFAULT)
	closemessage
	buffernumberstring(STR_VAR_2, 1250)
	if(!flag(FLAG_RECEIVED_CHOICE_BAND)) {dynmultipush("Choice Band",ITEM_CHOICE_BAND)}
    if(!flag(FLAG_RECEIVED_CHOICE_SCARF)) {dynmultipush("Choice Scarf",ITEM_CHOICE_SCARF)}
