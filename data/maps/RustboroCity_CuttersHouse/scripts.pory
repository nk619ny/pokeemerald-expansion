mapscripts RustboroCity_CuttersHouse_MapScripts {}

script RustboroCity_CuttersHouse_EventScript_Lass {
	lockall	
	msgbox(RustboroCity_CuttersHouse_Text_DadHelpedClearLandOfTrees, MSGBOX_NPC)
	releaseall
	end
}

script RustboroCity_CuttersHouse_EventScript_Cutter {
	lock
	msgbox(format("I see, your Pokémon already know how to cut down trees!\pIt's amazing all the things Pokémon can do."), MSGBOX_NPC)
	release
	end
}

text RustboroCity_CuttersHouse_Text_DadHelpedClearLandOfTrees {
	format("When they were expanding the city of RUSTBORO, my dad helped out.\pHe made his POKéMON use CUT to clear the land of trees.")
}