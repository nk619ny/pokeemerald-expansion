	setvar(TempVar_PlayerLocation,3)
	return
}

script SeafloorCavern_Room9_EventScript_PlayerArchieFaceToFace {
	switch(var(TempVar_PlayerLocation)){
		case 1:
			applymovement(LOCALID_ARCHIE, Common_Movement_WalkInPlaceFasterLeft)
			applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_FaceRight)
		case 2:
			applymovement(LOCALID_ARCHIE, Common_Movement_WalkInPlaceFasterDown)
			applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_FaceUp)
		case 3:
			applymovement(LOCALID_ARCHIE, Common_Movement_WalkInPlaceFasterRight)
			applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_FaceLeft)
		default:
	}
	waitmovement(0)
	return
}

script SeafloorCavern_Room9_EventScript_ArchieFacePlayer {
	switch(var(TempVar_PlayerLocation)){
		case 1:
			applymovement(LOCALID_ARCHIE, Common_Movement_WalkInPlaceFasterLeft)
		case 2:
			applymovement(LOCALID_ARCHIE, Common_Movement_WalkInPlaceFasterDown)
		case 3:
			applymovement(LOCALID_ARCHIE, Common_Movement_WalkInPlaceFasterRight)
		default:
	}
	waitmovement(0)
	return
}

script SeafloorCavern_Room9_EventScript_ArchieWalkMessage {
	switch(var(TempVar_PlayerLocation)){
		case 1:
			applymovement(LOCALID_ARCHIE, SeafloorCavern_Room9_Movement_ArchieListenMessage1)
		case 2:
		case 3:
			applymovement(LOCALID_ARCHIE, SeafloorCavern_Room9_Movement_ArchieListenMessage)
		default:
	}
	waitmovement(0)
	return
}

movement SeafloorCavern_Room9_Movement_KyogreNewExit {
	walk_fast_up * 10
}